{
  "name": "storybook-addon-grid",
  "version": "0.7.0",
  "description": "Column guides for your stories",
  "keywords": [
    "storybook",
    "storybook-addon",
    "columns",
    "grids",
    "guides",
    "design"
  ],
  "repository": "maraisr/storybook-addon-grid",
  "license": "MIT",
  "author": "Marais Rossouw <me@marais.dev> (https://marais.io)",
  "sideEffects": false,
  "exports": {
    ".": "./index.js",
    "./chromatic": "./chromatic.js",
    "./preview": "./preview.js",
    "./manager": "./manager.js",
    "./package.json": "./package.json"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "files": [
    "index.js",
    "index.d.ts",
    "chromatic.js",
    "preview.js",
    "manager.js",
    "README.md"
  ],
  "scripts": {
    "build": "bundt",
    "example": "pnpm --filter example storybook dev",
    "format": "prettier --write --list-different \"{*,{src,example,bin,.github}/**/*}.+(ts|tsx|js|json|yml|md)\"",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@marais/prettier",
  "devDependencies": {
    "@marais/prettier": "0.0.4",
    "@marais/tsconfig": "0.0.4",
    "@types/react-dom": "^19",
    "@types/react": "^19",
    "react-dom": "^19",
    "react": "^19",
    "bundt": "2.0.0-next.5",
    "chromatic": "11.28.2",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "storybook": ">=9.0.0-0 <10.0.0-0"
  },
  "peerDependencies": {
    "storybook": "^9",
    "@types/react-dom": ">=17",
    "@types/react": ">=17",
    "chromatic": "*",
    "react-dom": ">=17",
    "react": ">=17"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    },
    "chromatic": {
      "optional": true
    }
  },
  "storybook": {
    "displayName": "Column Guides",
    "icon": "https://raw.githubusercontent.com/maraisr/storybook-addon-grid/main/shots/square_logo.png",
    "unsupportedFrameworks": [
      "react-native"
    ]
  }
}
