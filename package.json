{
  "name": "storybook-addon-grid",
  "version": "0.5.2",
  "description": "Column guides for your stories",
  "keywords": [
    "storybook",
    "storybook-addon",
    "columns",
    "grids",
    "guides",
    "design"
  ],
  "repository": "maraisr/storybook-addon-grid",
  "license": "MIT",
  "author": "Marais Rossow <me@marais.dev> (https://marais.io)",
  "sideEffects": false,
  "exports": {
    ".": "./index.js",
    "./chromatic": {
      "import": "./chromatic.mjs",
      "require": "./chromatic.js"
    },
    "./preview": {
      "import": "./preview.mjs",
      "require": "./preview.js"
    },
    "./manager": {
      "import": "./manager.mjs",
      "require": "./manager.js"
    },
    "./package.json": "./package.json"
  },
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "files": [
    "index.mjs",
    "index.js",
    "index.d.ts",
    "chromatic.js",
    "chromatic.mjs",
    "preview.js",
    "preview.mjs",
    "manager.js",
    "manager.mjs",
    "README.md"
  ],
  "scripts": {
    "build": "bundt",
    "example": "pnpm --filter example storybook dev",
    "format": "prettier --write --list-different \"{*,{src,example,bin,.github}/**/*}.+(ts|tsx|js|json|yml|md)\"",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@marais/prettier",
  "dependencies": {
    "@storybook/components": "^8.5.2",
    "@storybook/core-events": "^8.5.2",
    "@storybook/manager-api": "^8.5.2",
    "@storybook/theming": "^8.5.2",
    "@storybook/types": "^8.5.2"
  },
  "devDependencies": {
    "@marais/prettier": "0.0.4",
    "@marais/tsconfig": "0.0.4",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "bundt": "2.0.0-next.5",
    "chromatic": "11.2.0",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.4.3"
  },
  "peerDependencies": {
    "@types/react": ">=16.9.0",
    "@types/react-dom": ">=16.9.0",
    "chromatic": "*",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    },
    "chromatic": {
      "optional": true
    }
  },
  "storybook": {
    "displayName": "Column Guides",
    "icon": "https://raw.githubusercontent.com/maraisr/storybook-addon-grid/main/shots/square_logo.png",
    "unsupportedFrameworks": [
      "react-native"
    ]
  }
}
